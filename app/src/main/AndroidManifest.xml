<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="slipinit.com.slip.in.slipitin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".slip_finder.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".slip_finder.activity.SlipFinderParentActivity"
            android:launchMode="singleTop">

        <meta-data
            android:name="com.google.android.gms.wallet.api.enabled"
            android:value="true" />
        </activity>
        <activity android:name=".stripe.PaymentActvity"></activity>
    </application>

</manifest>